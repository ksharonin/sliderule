{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/sliderule/targets/server-linux/sliderule",
        //"program": "/usr/local/bin/sliderule",
        //"args": ["${workspaceFolder}/scripts/selftests/test_runner.lua"],
        //"args": ["${workspaceFolder}/scripts/selftests/geojson_raster.lua"],
        "args": ["${workspaceFolder}/plugins/arcticdem/selftests/arcticdem_reader.lua"],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description":  "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
}